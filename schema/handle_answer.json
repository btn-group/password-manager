          "type": "object",
          "required": [
            "status"
          ],
          "properties": {
            "status": {
              "$ref": "#/definitions/ResponseStatus"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "show"
      ],
      "properties": {
        "show": {
          "type": "object",
          "required": [
            "authentication"
          ],
          "properties": {
            "authentication": {
              "$ref": "#/definitions/Authentication"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "update_authentication"
      ],
      "properties": {
        "update_authentication": {
          "type": "object",
          "required": [
            "authentication"
          ],
          "properties": {
            "authentication": {
              "$ref": "#/definitions/Authentication"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Authentication": {
      "type": "object",
      "required": [
        "label",
        "notes",
        "password",
        "position",
        "username"
      ],
      "properties": {
        "label": {
          "type": "string"
        },
        "notes": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "position": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "username": {
          "type": "string"
        }
      }
    },
    "ResponseStatus": {
      "type": "string",
      "enum": [
        "success"
      ]
    }
  }
